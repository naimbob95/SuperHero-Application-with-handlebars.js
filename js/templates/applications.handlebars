<div class="row">
   <div class="col-sm-12">
      <a class="btn btn-primary" href="#applications/addapplication" role="button">Add Application</a> 
     
      <p>&nbsp;</p>  
      <table class="table table-striped table-hover table-condensed" id="tbl1">
         <thead>
            <tr>
               <th>Index</th>
               <th>Superhero Name</th>
               <th>Mask</th>
               <th>Cape</th>
               <th>Costume</th>
               <th>Superhero Power</th>
               <th>Added</th>
               <th>Status</th>
               <th>Delete</th>
            </tr>
         </thead>
         <tbody>
{{#each contacts}}
            <tr>
               <td class='index-column'>{{ id }}</td>
               <td><a href="#applications/view/{{id}}">{{name}}</a></td>
               <td>{{ name }}</td>
               <td>{{ mask }}</td>
               <td>{{Cape}}</td>
               <td>{{costume}}</td>
               <td>{{superpower}}</td>
               <td><span class="label label-primary">{{ addeddate }}</span></td>
               <td>{{status}}</td>
               <td>
                  <a href="javascript:;">
                     {{!-- <i class="far fa-trash-alt"></i> --}}
                  <i class="fa fa-trash" aria-hidden="true"  data-contactid="{{ id }}"></i>
                     {{!-- <span class="glyphicon glyphicon-trash" aria-hidden="true" data-contactid="{{ id }}"></span> --}}
         {{/each}} 
         </tbody>
      </table>
   </div>
</div>