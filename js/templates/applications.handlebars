<div class="row">
   <div class="col-sm-12">
      <a class="btn btn-primary" href="#applications/addapplication" role="button">Add Application</a> 
     
      <p>&nbsp;</p>  
      <table class="table table-striped table-hover table-condensed" id="tbl2">
         <thead>
            <tr>
               <th>Index</th>
               <th>Superhero Name</th>
               <th>Mask</th>
               <th>Cape</th>
               <th>Costume</th>
               <th>Superhero Power</th>
               <th>Added</th>
               <th>Status</th>
               {{!-- <th>Delete</th> --}}
            </tr>
         </thead>
         <tbody>
{{#each applications}}
            <tr>
               <td class='index-column'>{{ id }}</td>
               <td><a href="#applications/view/{{id}}">{{name}}</a></td>
               <td>{{cape}}</td>
               <td>{{mask}}</td>
               <td>{{costume}}</td>
               <td>{{superpower}}</td>
               <td><span class="label label-info">{{ addeddate }}</span></td>
           <td>{{{displayverifyuser verify}}}</td>
                 
                {{!-- <td><a href="javascript:;"> --}}
                  
                  {{!-- <i class="fa fa-trash" aria-hidden="true"  data-applicationid="{{ id }}"></i></td> --}}
                    
         {{/each}} 
         </tbody>
      </table>
   </div>
</div>